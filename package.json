{
  "name": "orthogonal-array-test-online-tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "jest --no-cache",
    "test": "npm run lint && npm run test:unit"
